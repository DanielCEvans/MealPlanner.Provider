{
  "fido2": {
    "serverDomain": "localhost",
    "origins": [ "http://localhost:3001" ],
    "timestampDriftTolerance": 300000,
    "backupEligibleCredentialPolicy": "allowed",
    "backedUpCredentialPolicy": "allowed"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=meal_planner;Username=postgres;Password=postgres;",
    "ErrorDetailsConnection": "Host=localhost;Database=meal_planner;Username=postgres;Password=postgres;IncludeErrorDetail=true"
  }
}
